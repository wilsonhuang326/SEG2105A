<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <ImageView
        android:id="@+id/teamid00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toEndOf="@+id/teamid05"
        app:layout_constraintTop_toBottomOf="@+id/teamid03"
        app:srcCompat="@drawable/ic_logo_00" />

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Select a new Avatar"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/teamid02"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/teamid05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toEndOf="@+id/teamid04"
        app:layout_constraintTop_toBottomOf="@+id/teamid02"
        app:srcCompat="@drawable/ic_logo_05" />

    <ImageView
        android:id="@+id/teamid02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="140dp"
        app:layout_constraintStart_toEndOf="@+id/teamid01"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_02" />

    <ImageView
        android:id="@+id/teamid01"
        android:layout_width="121dp"
        android:layout_height="130dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="140dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_01" />

    <ImageView
        android:id="@+id/teamid03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="140dp"
        app:layout_constraintStart_toEndOf="@+id/teamid02"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_03" />

    <ImageView
        android:id="@+id/teamid04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teamid01"
        app:srcCompat="@drawable/ic_logo_04" />

</androidx.constraintlayout.widget.ConstraintLayout>